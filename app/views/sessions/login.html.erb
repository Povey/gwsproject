<% @page_title = "UserAuth | Login" -%>
<div class= "Sign_Form">
  <h1>Log in</h1>
  <%= form_for :session, :url => sessions_path, method: "POST" do %>
    <p>Username or Email:</br> <%= text_field_tag(:username) %></p>
    <p>Password:</br> <%= password_field_tag :login_password %></p>
    <%= submit_tag("Log In") %>
<% end %>
</div>
